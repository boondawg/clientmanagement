<% provide(:title, 'Create Consultation Type') %>
<h1>Create Consultation Type</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@consultation_type) do |f| %>

        <%= f.label :name %>
        <%= f.text_field :name %>

        <%= f.label :cost %>
        <%= f.text_field :cost %>

        <%= f.submit "Create new consultation type", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>