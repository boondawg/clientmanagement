<% provide(:title, 'Create Note Template') %>
<h1>Create Consultation Type</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@note_template) do |f| %>

        <%= f.label :name %>
        <%= f.text_field :name %>
        <%= f.fields_for :note_template_field do |n| %>
        <%= n.label :name%>
            <%= n.text_field :name %>

        <%end%>



        <%= f.submit 'Create new note template', class: 'btn blue' %>
    <% end %>
  </div>
</div>
