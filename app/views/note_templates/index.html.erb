<h1>Listing note_templates</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @note_templates.each do |note_template| %>
      <tr>
        <td><%= link_to 'Show', note_template %></td>
        <td><%= link_to 'Edit', edit_note_template_path(note_template) %></td>
        <td><%= link_to 'Destroy', note_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

